entrypoint = "index.js"
modules = ["nodejs-22", "python-3.11"]
hidden = [".config", "package-lock.json"]

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "package.json", "package-lock.json"]

[nix]
channel = "stable-24_11"
packages = ["undollar", "wget", "zulu"]

[deployment]
run = "node index.js"

[[ports]]
localPort = 3000
externalPort = 80

[[ports]]
localPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 9005

[[ports]]
localPort = 9006
externalPort = 3000

[[ports]]
localPort = 9007
externalPort = 3001

[agent]
expertMode = true

[workflows]

[[workflows.workflow]]
name = "ASL Service"
mode = "sequential"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node ASL.js"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Webhook Service"
mode = "sequential"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node index.js"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Documentation Viewer(Deprecated)"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -c \"import os; print('LLD文檔已生成，位置：82. Flutter_LLD_APL/'); print('已完成文檔：'); [print(f'- {f}') for f in os.listdir('82. Flutter_LLD_APL') if f.endswith('.md')]; import time; time.sleep(30)\""

[workflows.workflow.metadata]
outputType = "console"
